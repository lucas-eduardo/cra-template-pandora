{
  "files.exclude": {
    "node_modules": false,
    "package-lock.json": false,
    "tsconfig.json": false,
    "config-overrides.js": false,
    ".gitignore": false,
    ".editorconfig": false,
    ".eslint*": false
  },
  "editor.rulers": [ 80 ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  }
}
